msgstr ""

#: ../../running.rst:43
msgid "runs the command line user interface"
msgstr "コマンドラインユーザインタフェースの実行"

#: ../../running.rst:46
msgid "-version:"
msgstr ""

#: ../../running.rst:46
msgid "displays the SpotBugs version number"
msgstr "SpotBugs のバージョン番号の表示"

#: ../../running.rst:49
msgid "-help:"
msgstr ""

#: ../../running.rst:49
